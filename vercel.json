{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/chat",
      "dest": "/public/chat.html"
    },
    {
      "src": "/home",
      "dest": "/public/home.html"
    },
    {
      "src": "/messages",
      "dest": "/public/messages.html"
    },
    {
      "src": "/profile",
      "dest": "/public/profile.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
