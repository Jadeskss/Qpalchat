{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/home",
      "dest": "/public/home.html"
    },
    {
      "src": "/chat",
      "dest": "/public/chat.html"
    },
    {
      "src": "/messages",
      "dest": "/public/messages.html"
    },
    {
      "src": "/profile",
      "dest": "/public/profile.html"
    },
    {
      "src": "/profile-setup",
      "dest": "/public/profile-setup.html"
    },
    {
      "src": "/auth-callback",
      "dest": "/public/auth-callback.html"
    },
    {
      "src": "/admin-login",
      "dest": "/public/admin-login.html"
    },
    {
      "src": "/admin-dashboard",
      "dest": "/public/admin-dashboard.html"
    },
    {
      "src": "/admin",
      "dest": "/public/admin-login.html"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json))",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "public/**/*.js": {
      "runtime": "@vercel/node@18"
    }
  }
}
