{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/index",
      "dest": "/public/index.html"
    },
    {
      "src": "/home.html",
      "dest": "/public/home.html"
    },
    {
      "src": "/home",
      "dest": "/public/home.html"
    },
    {
      "src": "/chat.html",
      "dest": "/public/chat.html"
    },
    {
      "src": "/chat",
      "dest": "/public/chat.html"
    },
    {
      "src": "/messages.html",
      "dest": "/public/messages.html"
    },
    {
      "src": "/messages",
      "dest": "/public/messages.html"
    },
    {
      "src": "/profile.html",
      "dest": "/public/profile.html"
    },
    {
      "src": "/profile",
      "dest": "/public/profile.html"
    },
    {
      "src": "/profile-setup.html",
      "dest": "/public/profile-setup.html"
    },
    {
      "src": "/profile-setup",
      "dest": "/public/profile-setup.html"
    },
    {
      "src": "/auth-callback.html",
      "dest": "/public/auth-callback.html"
    },
    {
      "src": "/auth-callback",
      "dest": "/public/auth-callback.html"
    },
    {
      "src": "/admin-login.html",
      "dest": "/public/admin-login.html"
    },
    {
      "src": "/admin-login",
      "dest": "/public/admin-login.html"
    },
    {
      "src": "/admin-dashboard.html",
      "dest": "/public/admin-dashboard.html"
    },
    {
      "src": "/admin-dashboard",
      "dest": "/public/admin-dashboard.html"
    },
    {
      "src": "/admin",
      "dest": "/public/admin-login.html"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
